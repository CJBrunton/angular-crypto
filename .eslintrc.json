{
	"extends": ["plugin:prettier/recommended", "eslint:recommended", "plugin:angular/johnpapa"],
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"globals": {
		"angular": false,
		"mainApp":false
    },
	"env": {
		"es6": true,
		"browser": true
	},
	"plugins": ["angular"],
	"rules": {	
		"no-console": "off", 
		"angular/module-getter": "off",
		"angular/controller-as":"off",
		"angular/function-type":"off"
	}
}